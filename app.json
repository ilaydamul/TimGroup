{
  "expo": {
    "name": "TimGroup",
    "slug": "TimGroup",
    "version": "1.0.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "5",
      "supportsTablet": false,
      "bundleIdentifier": "tr.com.timgroup"
    },
    "android": {
      "versionCode": 5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "package": "tr.com.timgroup",
      "allowBackup": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uygulama, denetim fotoğrafı seçebilmeniz amacıyla galerinize erişim istiyor",
          "cameraPermission": "Uygulama, denetim fotoğrafı kaydedebilmeniz amacıyla kameranıza erişim istiyor."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Uygulama, denetim fotoğrafı kaydedebilmeniz amacıyla kameranıza erişim istiyor",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Uygulama, denetim sahanızı denetlemek için konum bilgilerinize erişim istiyor."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d47c5327-959f-4bc1-b26d-dd26114b8258"
      }
    }
  }
}